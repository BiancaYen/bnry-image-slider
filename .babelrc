{
  "env": {
    "production": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
      ],
      "plugins": ["emotion"]
    },
    "development": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
      ],
      "plugins": [
        ["emotion", { "sourceMap": true, "autoLabel": true }],
        ["@babel/plugin-syntax-dynamic-import"],
        ["@babel/transform-arrow-functions"],
        ["@babel/plugin-proposal-class-properties"]
      ]
    },
    "test": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
      ],
      "plugins": [
        ["emotion", { "sourceMap": true, "autoLabel": true }],
        ["@babel/plugin-syntax-dynamic-import"],
        ["@babel/transform-arrow-functions"],
        ["@babel/plugin-proposal-class-properties"]
      ]
    }
  }
}
